<odoo>
    <record model="ir.ui.view" id="view_account_journal_form">
        <field name="name">account.journal.form (in edi_account)</field>
        <field name="model">account.journal</field>
        <field name="inherit_id" ref="account.view_account_journal_form" />
        <field name="arch" type="xml">
            <group name="column_posting" position="after">
                <group name="edi_format" string="Electronic Documents">
                    <field name="account_move_edi_format_ids" widget="many2many_tags" />
                </group>
            </group>
        </field>
    </record>
</odoo>
