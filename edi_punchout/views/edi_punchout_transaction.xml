<odoo>

<record id="view_edi_punchout_transaction_form" model="ir.ui.view">
    <field name="model">edi.punchout.transaction</field>
    <field name="arch" type="xml">
        <form>
            <group>
                <field name="account_id" />
                <field name="transaction_id" />
                <field name="client_key" />
                <field name="exception" />
                <field name="request" />
            </group>
        </form>
    </field>
</record>

<record id="view_edi_punchout_transaction_tree" model="ir.ui.view">
    <field name="model">edi.punchout.transaction</field>
    <field name="arch" type="xml">
        <tree>
            <field name="account_id" />
            <field name="transaction_id" />
            <field name="client_key" />
        </tree>
    </field>
</record>

<act_window
        id="action_edi_punchout_transaction"
        name="Punchout transactions"
        res_model="edi.punchout.transaction"
        view_mode="tree,form"
    />

<menuitem
        id="menu_edi_punchout_transaction"
        action="action_edi_punchout_transaction"
        parent="base.menu_custom"
    />
</odoo>
