<odoo>
    <record id="voxel_login_form_view" model="ir.ui.view">
        <field name="name">voxel.login.view.form</field>
        <field name="model">voxel.login</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="url"/>
                        </group>
                        <group>
                            <field name="user"/>
                            <field name="password" password="True"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
