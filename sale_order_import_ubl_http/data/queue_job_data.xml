<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">
        <record id="channel_ubl_import" model="queue.job.channel">
            <field name="name">ubl_import</field>
            <field name="parent_id" ref="queue_job.channel_root" />
        </record>
        <record id="job_import_ubl_from_http" model="queue.job.function">
            <field name="model_id" ref="sale.model_sale_order" />
            <field name="method">import_ubl_from_http</field>
            <field name="channel_id" ref="channel_ubl_import" />
            <field name="related_action" eval='{"func_name": "import_ubl_from_http"}' />
            <field name="retry_pattern" eval="{1: 60, 2: 180, 3: 10, 5: 300}" />
        </record>
    </data>
</odoo>
