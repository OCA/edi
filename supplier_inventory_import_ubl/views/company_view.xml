<odoo>
<data>


<record id="res_company_form_view" model="ir.ui.view">
    <field name="model">res.company</field>
    <field name="inherit_id" ref="base.view_company_form"/>
    <field name="arch" type="xml">
        <xpath expr="//page[last()]" position="after">
            <group name="ubl_inventory" string="Inventory Report" colspan="2">
                <field name="inventory_ubl_alert_partner_id" />
                <field name="inventory_ubl_store_document" />
                <field name="inventory_ubl_link_document" />
            </group>
        </xpath>
    </field>
</record>


</data>
</odoo>
